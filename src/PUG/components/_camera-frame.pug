mixin camera-frame
  .camera-frame
    .social-widget.social-widget__camera-frame
      #social-widget.animate__animated
        span#social-name
        i#social-icon
      +bolts

mixin camera-frame__large
  .camera-frame.camera-frame__large
    .social-widget.social-widget__camera-frame.social-widget__camera-frame__large
      span EDarkMatter
      i(class="fa-brands fa-youtube youtube")
      +bolts

mixin bolts
  .bolts
    div
    div